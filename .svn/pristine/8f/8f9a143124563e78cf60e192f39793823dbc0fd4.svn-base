@charset "utf-8";
/* CSS Document */

/*清除默认样式，设置自定义属性*/

html,body{width:100%;height:100%;background:#f8f8f8;}
*{
	margin:0;
	padding:0;
	box-sizing:inherit;
	-moz-box-sizing:inherit; /* Firefox */
	-webkit-box-sizing:inherit; /* Safari */
}

body{
	font-size:14px;
	font-family:"微软雅黑","Times New Roman",Arial, Helvetica, sans-serif;
	color:#333;
	box-sizing:border-box;
	-moz-box-sizing:border-box; /* Firefox */
	-webkit-box-sizing:border-box; /* Safari */
}
table{border-collapse:collapse;border-spacing:0;}
ul,ol{list-style:none;}
img{border:0;}
input,text,button,radio,checkbox,select,password,textarea{border:0;outline:none;}

.clearfix:after{clear:both;content:'';display:block;width: 0;height: 0;visibility:hidden;}
.clearfix{zoom:1;}

h1,h2,h3,h4{font-weight:normal;}
a{text-decoration:none;outline:none;blr:expression(this.onFocus=this.blur());}
a:focus{outline:none;}	
a:hidefocus{outline:none;}

a:link,a:visited,a:hover,a:active{text-decoration:none;}
a,
a:link,
a:visited,
a:hover,
a:active {color:inherit;zoom:1}
a {color:inherit}

.wrap{width:1130px;margin:0 auto;}
.L{float:left;}
.R{float:right;}
.hide{display:none;}
.show{display:block;}
.db{display:block;}
.dn{display:none;}
.gray{
	color:#949494;
}
.yellow{
	color:#e49023;
}
.green{
	color:#39cb99;
}
.blue{
	color:#3d94d6;
}
.mb17{
	margin-bottom:17px;
}
.mb10{
	margin-bottom:10px;
}
.bold{font-weight:bold;}
.fn{font-weight:normal;}

/*文字*/
.f12{font-size:12px;}
.f13{font-size:13px;}
.f14{font-size:14px;}
.f15{font-size:15px;} 			
.f16{font-size:16px;}
.f18{font-size:18px;}
.f20{font-size:20px;}
.f21{font-size:21px;}
.f22{font-size: 22px;}
.f24{font-size: 24px;}
.f32{font-size: 32px;}
.f34{font-size: 34px;}
.f30{font-size:30px;}

/*边距*/
.m10{margin:10px;}
.m15{margin:15px;}
.m20{margin:20px;}
.m30{margin:30px;}

.mr10{margin-right:10px;}
.mr20{margin-right:20px;}
.mr30{margin-right:30px;}
.mr50{margin-right:50px;}
.mr100{margin-right:100px;}

.mb10{margin-bottom:10px;}
.mb20{margin-bottom:20px;}
.mb30{margin-bottom:30px;}
.mb50{margin-bottom:50px;}
.mb100{margin-bottom:100px;}
.mt35{
	margin-top:35px;
}
.p10{padding:10px;}
.p20{padding:20px;}
.p30{padding:30px;}
.p50{padding:50px;}

.pl10{padding-left:10px;}
.pl20{padding-left:20px;}
.pl30{padding-left:30px;}
.pl50{padding-left:50px;}

/*定位 对齐*/
.pr{position:relative;}
.pa{position:absolute;}
.tl{text-align:left;}
.tc{text-align:center;}
.tr{text-align:right;}
.ti{text-align:inherit;}

.vm{vertical-align: middle;}
.vt{vertical-align: top;}
.vb{vertical-align: bottom;}
/*边框*/
.bd{border:1px solid #dbdbdb;}




